     SEQUENCE_DEFINITION (
       NAME ( Join_flash )
     
      ui::UI_COMMAND (
        NODE ( MENU  RTGL )
      
       CAPTION ( "RETURNING TO LOBBY." )
      
      )
     
      WAIT ( 0.300000 )
     
