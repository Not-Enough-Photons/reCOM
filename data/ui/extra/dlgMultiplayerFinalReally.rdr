    ANIMATION_DEFINITION (
      ANIMATION_NAME ( ExitOnStart )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( IDFromBriefing  =  0 )
     
      IF (
        ui::UI_COMMAND (
         TYPE ( HaveBeenBannedFromGame )
       
       )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( BanSelfFromGame )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( SetDefaultMenuScreen )
      
       ARGUMENT ( dlgNetBanned.rdr )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( GameStatePopCommand )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( SwitchMenu )
      
       ARGUMENT ( MENU_SCREEN )
      
      )
     
      ELSE ()
     
      IF (
        ! (
         ui::UI_COMMAND (
          TYPE ( IsSessionMaster )
        
        )
       
       )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( CloseAndSwitch )
      
       ARGUMENT ( dlgGameLobby.rdr )
      
      )
     
      CALL_SEQUENCE (
        NAME ( watch_for_switch_to_master )
      
      )
     
      ELSE ()
     
      CALL_SEQUENCE (
        NAME ( watch_for_switch_to_client )
      
      )
     
      ENDIF ()
     
      ENDIF ()
     
     )
    
     SEQUENCE_DEFINITION (
       NAME ( watch_for_switch_to_master )
     
      ACTIVATION ( ON_CALL )
     
      WHILE ( TRUE )
     
      IF ((
         ui::UI_COMMAND (
          TYPE ( IsSessionMaster )
        
        )
       
       )
      
      )
     
      CALL_SEQUENCE (
        NAME ( watch_for_switch_to_client )
      
      )
     
      BREAK ()
     
      ENDIF ()
     
      WAIT ( 1.000000 )
     
      END_WHILE ()
     
     )
    
     SEQUENCE_DEFINITION (
       NAME ( watch_for_switch_to_client )
     
      ACTIVATION ( ON_CALL )
     
      CALL_ANIMATION (
        NAME ( call_gamelobby )
      
      )
     
      WHILE ( TRUE )
     
      IF (
        ! (
         ui::UI_COMMAND (
          TYPE ( IsSessionMaster )
        
        )
       
       )
      
      )
     
      CALL_SEQUENCE (
        NAME ( watch_for_switch_to_master )
      
      )
     
      BREAK ()
     
      ENDIF ()
     
      WAIT ( 1.000000 )
     
      END_WHILE ()
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( call_gamelobby )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       WAIT ( 10 )
     
      SOUND (
        NAME ( SND_THUNK )
      
       START ()
      
      )
     
      CALL_ANIMATION (
        NAME ( return_to_gamelobby )
      
      )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( return_to_gamelobby )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       SOUND (
        NAME ( SND_THUNK )
      
       START ()
      
      )
     
      STOP_ANIMATION (
        NAME ( ExitOnStart )
      
      )
     
      VALVE ( player_ready_count  =  0 )
     
      VALVE ( mp_x_sec_clock  =  11 )
     
      ui::UI_COMMAND (
        TYPE ( CloseAndSwitch )
      
       ARGUMENT ( dlgGameLobby.rdr )
      
      )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( MaintainDoneCounting )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( DoneCounting  =  0 )
     
      WAIT ( 5.000000 )
     
      VALVE ( DoneCounting  =  1 )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( MpFinalOnOk )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       IF (
        VALVE ( DoneCounting  ==  1 )
      
      )
     
      CALL_ANIMATION (
        NAME ( return_to_gamelobby )
      
      )
     
      ELSE ()
     
      ui::UI_COMMAND (
        NODE ( MENU  SealsTotals )
      
       FINISH ()
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  TerrsTotals )
      
       FINISH ()
      
      )
     
      VALVE ( DoneCounting  =  1 )
     
      ENDIF ()
     
     )
    
    )
   
