      )
     
      ENDIF ()
     
      ENDIF ()
     
     )
    
     SEQUENCE_DEFINITION (
       NAME ( watch_for_switch_to_master )
     
      ACTIVATION ( ON_CALL )
     
      WHILE ( TRUE )
     
      IF ((
         ui::UI_COMMAND (
          TYPE ( IsSessionMaster )
        
        )
       
       )
      
      )
     
      CALL_SEQUENCE (
        NAME ( watch_for_switch_to_client )
      
      )
     
      BREAK ()
     
