        )
       
       )
      
      )
     
      CALL_SEQUENCE (
        NAME ( watch_for_switch_to_master )
      
      )
     
      BREAK ()
     
      ENDIF ()
     
      WAIT ( 1.000000 )
     
      END_WHILE ()
     
     )
    
