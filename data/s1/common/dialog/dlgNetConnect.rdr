    ANIMATION_DEFINITION (
      ANIMATION_NAME ( init_socom_online )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( UsagePolicyReturnScreen  =  1 )
     
      IF (
        VALVE ( socom_online_initialized  ==  0 )
      
      )
     
      VALVE ( socom_online_state  =  0 )
     
      IF (
        ui::UI_COMMAND (
         TYPE ( INITNETWORK )
       
       )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Waiting to Connect" )
      
      )
     
      WHILE (
        VALVE ( socom_online_state  ==  0 )
      
      )
     
      END_WHILE ()
     
      IF (
        VALVE ( socom_online_state  ==  1 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Connected" )
      
      )
     
      VALVE ( socom_online_initialized  =  1 )
     
      CALL_SEQUENCE (
        NAME ( socom_online_login )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  2 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Connect Failure" )
      
      )
     
      CALL_ANIMATION (
        NAME ( notify_connect_failure )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ELSE ()
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Unknown Failure" )
      
      )
     
      CALL_ANIMATION (
        NAME ( notify_unknown_failure )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ENDIF ()
     
      ELSE ()
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Network Initialization Failure" )
      
      )
     
      WAIT ( 1.000000 )
     
      ui::UI_COMMAND (
        TYPE ( SWITCHMENU )
      
       ARGUMENT ( PREVIOUS_SCREEN )
      
      )
     
      ENDIF ()
     
      ELSE ()
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Invalid Initialization State" )
      
      )
     
      WAIT ( 1.000000 )
     
      ui::UI_COMMAND (
        TYPE ( SWITCHMENU )
      
       ARGUMENT ( PREVIOUS_SCREEN )
      
      )
     
      ENDIF ()
     
     )
    
     SEQUENCE_DEFINITION (
       NAME ( socom_online_login )
     
      ACTIVATION ( ON_CALL )
     
      IF (
        ui::UI_COMMAND (
         TYPE ( LobbyLogin )
       
       )
      
      )
     
      WHILE ( TRUE )
     
      IF (
        VALVE ( socom_online_state  ==  12 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Session Begin" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  7 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Account Login" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  3 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Account Registration" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  8 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: User Login" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  4 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: User Registration" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  9 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Screename Login" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  5 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Screename Registration" )
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  10 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Login Failure" )
      
      )
     
      VALVE ( AreYouSure  =  0 )
     
      STOP_ANIMATION (
        NAME ( menu_fade_up )
      
      )
     
      OBJECT_OPACITY_FROM_TO (
        NAME ( MENU  MENUFADER )
      
       RUN_TIME ( 0.050000 )
      
       OPACITY_FROM ( 0.000000 )
      
       OPACITY_TO ( 0.700000 )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( POPUPDIALOG )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  POPUP )
      
       CAPTION ( "Player Name not found. Would you like to register this Player Name?" )
      
       BACKGROUND ( popupOp.tif )
      
       POSITION ( 170.000000  240.000000 )
      
       WIDTH ( 300 )
      
       HEIGHT ( 125 )
      
       XOFFSET ( 20 )
      
       YOFFSET ( 30 )
      
       SPACING ( 20 )
      
       SCALE ( 0.400000 )
      
       YESNO (
         VALVE ( AreYouSure )
       
        YES (
          CAPTION ( Yes )
        
         POS ( 60  90 )
        
         TEXTOFFSET ( 10  18 )
        
         WIDTH ( 50 )
        
         HEIGHT ( 25 )
        
         PRESSED (
           IMAGE ( pop_butt.tif )
         
          TEXT_COLOR ( 100  0  0 )
         
         )
        
         ACTIVE (
           IMAGE ( pop_butt.tif )
         
          TEXT_COLOR ( 255  204  51 )
         
         )
        
         NORMAL (
           IMAGE ( pop_butt.tif )
         
          TEXT_COLOR ( 128  128  128 )
         
         )
        
        )
       
        NO (
          CAPTION ( No )
        
         POS ( 180  90 )
        
         TEXTOFFSET ( 15  18 )
        
         WIDTH ( 50 )
        
         HEIGHT ( 25 )
        
         PRESSED (
           IMAGE ( pop_butt.tif )
         
          TEXT_COLOR ( 100  0  0 )
         
         )
        
         ACTIVE (
           IMAGE ( pop_butt.tif )
         
          TEXT_COLOR ( 255  204  51 )
         
         )
        
         NORMAL (
           IMAGE ( pop_butt.tif )
         
          TEXT_COLOR ( 128  128  128 )
         
         )
        
        )
       
       )
      
      )
     
      WHILE (
        VALVE ( AreYouSure  ==  0 )
      
      )
     
      END_WHILE ()
     
      OBJECT_OPACITY_FROM_TO (
        NAME ( MENU  MENUFADER )
      
       RUN_TIME ( 0.050000 )
      
       OPACITY_FROM ( 0.700000 )
      
       OPACITY_TO ( 0.000000 )
      
      )
     
      IF (
        VALVE ( AreYouSure  ==  1 )
      
      )
     
      IF (
        ! (
         ui::UI_COMMAND (
          TYPE ( LobbyRegister )
        
        )
       
       )
      
      )
     
      CALL_ANIMATION (
        NAME ( exit_connect_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ENDIF ()
     
      ELSE ()
     
      CALL_ANIMATION (
        NAME ( exit_connect_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ENDIF ()
     
      ELSEIF (
        VALVE ( socom_online_state  ==  6 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Registration Failure" )
      
      )
     
      WAIT ( 1.000000 )
     
      CALL_ANIMATION (
        NAME ( exit_connect_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ELSEIF (
        VALVE ( socom_online_state  ==  11 )
      
      )
     
      BREAK ()
     
      ENDIF ()
     
      END_WHILE ()
     
      IF (
        VALVE ( socom_online_state  ==  11 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Logged In" )
      
      )
     
      WAIT ( 2.000000 )
     
      ui::UI_COMMAND (
        TYPE ( SWITCHMENU )
      
       ARGUMENT ( dlgUsagePolicy.rdr )
      
      )
     
      ENDIF ()
     
      ELSE ()
     
      CALL_ANIMATION (
        NAME ( exit_connect_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ENDIF ()
     
     )
    
    )
   
