      ANIMATION_NAME ( refresh_combo )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       IF (
        VALVE ( SELECTED_COMBO  ==  1 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  combo_value )
      
       CAPTION ( "Combination 1" )
      
      )
     
      ELSEIF (
        VALVE ( SELECTED_COMBO  ==  2 )
      
