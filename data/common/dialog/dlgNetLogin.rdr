     ui::UI_COMMAND(
       TYPE(KILLPOPUP)
      
      )
     
     ui::UI_COMMAND(
       TYPE(ActivateButton)
      
      ARGUMENT(login_btn)
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUDescriptionText)
      
      CAPTION("Login to SOCOM to play online")
      
      )
     
     ui::UI_COMMAND(
       TYPE(PasswordSecurity)
      
      )
     
     ENDIF()
     
     )
    
