    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(SELECTED_COMBO+= 1  0.000000 )
     
     IF(
       VALVE(SELECTED_COMBO> 6  0.000000 )
      
