   ANIMATION_DEFINITION(
     ANIMATION_NAME(OnLoginRoomstart)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(CameFromOther= 0  0.000000 )
     
     ui::UI_COMMAND(
       TYPE(MC_Init)
      
      )
     
     ui::UI_COMMAND(
       TYPE(POPUPDIALOG)
      
      )
     
     IF(
       ui::UI_COMMAND(
        TYPE(IsMemCardInserted)
       
       )
      
      &&ui::UI_COMMAND(
        TYPE(IsMemCardFormatted)
       
       )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUPOPUP)
      
      CAPTION("Loading network configuration from memory card (PS2). Do not remove memory card (PS2).")
      
      PROTECTED()
      
      BACKGROUND(popupOp.tif)
      
      POSITION( 170.000000  275.000000 )
      
      TEXT_COLOR( 255  0.000000  204  0.000000  51  0.000000 )
      
      WIDTH( 300  0.000000 )
      
      HEIGHT( 96  0.000000 )
      
      XOFFSET( 20  0.000000 )
      
      YOFFSET( 35  0.000000 )
      
      SPACING( 20  0.000000 )
      
      SCALE( 0.450000 )
      
      )
     
     ELSE()
     
     IF(
       VALVE(NETCNF_REQUIRED!= 0  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       TYPE(DisableButton)
      
      ARGUMENT(login_btn)
      
      )
     
     ENDIF()
     
     ui::UI_COMMAND(
       NODE(MENUPOPUP)
      
      CAPTION("Checking for network adapter")
      
      PROTECTED()
      
      BACKGROUND(popupOp.tif)
      
      POSITION( 170.000000  275.000000 )
      
      TEXT_COLOR( 255  0.000000  204  0.000000  51  0.000000 )
      
      WIDTH( 300  0.000000 )
      
      HEIGHT( 96  0.000000 )
      
      XOFFSET( 20  0.000000 )
      
      YOFFSET( 35  0.000000 )
      
      SPACING( 20  0.000000 )
      
      SCALE( 0.450000 )
      
      )
     
     ENDIF()
     
     ui::UI_COMMAND(
       TYPE(NetCnfInit)
      
      )
     
     ui::UI_COMMAND(
       TYPE(NetCnfOpen)
      
      ARGUMENT(mc0:)
      
      )
     
     ui::UI_COMMAND(
       TYPE(NetGetInterfaceList)
      
      )
     
     IF(
       !(
        ui::UI_COMMAND(
         TYPE(TestUIVar)
        
        ARGUMENT(NETDEVICE_LIST)
        
        )
       
       )
      
      )
     
     ui::UI_COMMAND(
       TYPE(POPUPDIALOG)
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUPOPUP)
      
      CAPTION("No network adapter detected. Online play requires a network adapter.")
      
      BACKGROUND(popupOp.tif)
      
      POSITION( 170.000000  275.000000 )
      
      TEXT_COLOR( 255  0.000000  204  0.000000  51  0.000000 )
      
      WIDTH( 300  0.000000 )
      
      HEIGHT( 110  0.000000 )
      
      XOFFSET( 20  0.000000 )
      
      YOFFSET( 35  0.000000 )
      
      SPACING( 20  0.000000 )
      
      SCALE( 0.400000 )
      
      YESNO(
        VALVE(NeedNetAdapter)
       
       )
      
      )
     
     WHILE(
       VALVE(NeedNetAdapter== 0  0.000000 )
      
      )
     
     END_WHILE()
     
     ui::UI_COMMAND(
       TYPE(NetCnfClose)
      
      )
     
     ui::UI_COMMAND(
       TYPE(NetCnfUnInit)
      
      )
     
     ui::UI_COMMAND(
       TYPE(MC_UnInit)
      
      )
     
     ui::UI_COMMAND(
       TYPE(RebootIOP)
      
      )
     
     ui::UI_COMMAND(
       TYPE(SWITCHMENU)
      
      ARGUMENT(PREVIOUS_SCREEN)
      
      )
     
     VALVE(net_config_is_init= 0  0.000000 )
     
     ELSE()
     
     ui::UI_COMMAND(
       TYPE(NetCnfCurrCombo)
      
      ARGUMENT(SELECTED_COMBO)
      
      )
     
     CALL_ANIMATION(
       NAME(refresh_combo)
      
