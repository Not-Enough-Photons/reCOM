      ELSE ()
     
      VALVE ( Current_X  +=  -1 )
     
      ENDIF ()
     
      CALL_ANIMATION (
        NAME ( refresh_icon_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( MoveRight )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( Current_X  +=  1 )
     
      IF (
        VALVE ( Current_X  >  Max_x )
      
      )
     
      VALVE ( Current_X  =  0 )
     
      ENDIF ()
     
      CALL_ANIMATION (
        NAME ( refresh_icon_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
     )
    
    )
   
