    ANIMATION_DEFINITION (
      ANIMATION_NAME ( refresh_icon_screen )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( back_x  =  Current_X )
     
      VALVE ( back_x  *=  X_Width )
     
      VALVE ( back_x  +=  Start_X )
     
      VALVE ( back_y  =  Current_Y )
     
      VALVE ( back_y  *=  Y_Width )
     
      VALVE ( back_y  +=  Start_Y )
     
      VALVE ( back_z  =  0 )
     
      OBJECT_MOTION_FROM_TO (
        NAME ( MENU  IconDummy )
      
       TRANSLATE_TO (
         VALVES ( back_x  back_y  back_z )
       
       )
      
       RUN_TIME ( 0.010000 )
      
      )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( MoveUp )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       IF (
        VALVE ( Current_Y  ==  0 )
      
      )
     
      VALVE ( Current_Y  =  Max_y )
     
      ELSE ()
     
      VALVE ( Current_Y  +=  -1 )
     
      ENDIF ()
     
      CALL_ANIMATION (
        NAME ( refresh_icon_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( MoveDown )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( Current_Y  +=  1 )
     
      IF (
        VALVE ( Current_Y  >  Max_y )
      
      )
     
      VALVE ( Current_Y  =  0 )
     
      ENDIF ()
     
      CALL_ANIMATION (
        NAME ( refresh_icon_screen )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
     )
    
    )
   
    ANIMATION_DEFINITION (
