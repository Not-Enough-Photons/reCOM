    ANIMATION_DEFINITION (
      ANIMATION_NAME ( check_loop )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       IF (
        VALVE ( GoneToArmory  ==  0 )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( AdvanceMPMission )
      
      )
     
      ENDIF ()
     
      VALVE ( GoneToArmory  =  0 )
     
      ui::UI_COMMAND (
        TYPE ( ForceMediusStatusReport )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( SETMENUSTATE )
      
      )
     
      WHILE (
        VALVE ( mp_major_game_state  !=  2 )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( CheckForcedLaunch )
      
      )
     
      CALL_ANIMATION (
        NAME ( CountDown )
      
      )
     
      END_WHILE ()
     
      ui::UI_COMMAND (
        TYPE ( ForceMediusStatusReport )
      
      )
     
      STOP_ANIMATION (
        NAME ( menu_fade_up )
      
      )
     
      OBJECT_OPACITY_FROM_TO (
        NAME ( MENU  MENUFADER )
      
       RUN_TIME ( 0.500000 )
      
       OPACITY_FROM ( 0.000000 )
      
       OPACITY_TO ( 0.990000 )
      
      )
     
      ui::UI_COMMAND (
