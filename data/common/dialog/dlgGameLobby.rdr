     SEQUENCE_DEFINITION (
       IF ((
         ui::UI_COMMAND (
          TYPE ( IsSessionMaster )
        
        )
       
       )
      
      )
     
      IF (
        VALVE ( SeenPopup  ==  0 )
      
      )
     
      STOP_ANIMATION (
        NAME ( menu_fade_up )
      
      )
     
      OBJECT_OPACITY_FROM_TO (
        NAME ( MENU  MENUFADER )
      
       RUN_TIME ( 0.050000 )
      
       OPACITY_FROM ( 0.000000 )
      
       OPACITY_TO ( 0.700000 )
      
      )
     
      VALVE ( HostValve  =  0 )
     
      ui::UI_COMMAND (
        TYPE ( POPUPDIALOG )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  POPUP )
      
       CAPTION ( "You are the Host of this game. As soon as we get word from the Pentagon, The FORCE LAUNCH and READY buttons will be available. Press the X button to continue." )
      
       BACKGROUND ( popupOp.tif )
      
       POSITION ( 195.000000  230.000000 )
      
       WIDTH ( 280 )
      
       HEIGHT ( 130 )
      
       XOFFSET ( 20 )
      
       YOFFSET ( 30 )
      
       SCALE ( 0.400000 )
      
       YESNO (
         VALVE ( HostValve )
       
       )
      
      )
     
      WHILE (
        VALVE ( HostValve  ==  0 )
      
      )
     
      WAIT ( 0.500000 )
     
      END_WHILE ()
     
      VALVE ( SeenPopup  =  1 )
     
      ui::UI_COMMAND (
        TYPE ( KillPopup )
      
      )
     
      OBJECT_OPACITY_FROM_TO (
        NAME ( MENU  MENUFADER )
      
       RUN_TIME ( 0.050000 )
      
       OPACITY_FROM ( 0.700000 )
      
       OPACITY_TO ( 0.000000 )
      
      )
     
      ENDIF ()
     
      ENDIF ()
     
     )
    
