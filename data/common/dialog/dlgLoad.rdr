       NAME(MENURedDown)
      
      TRANSLATE_FROM( -4.000000  -50.000000  0.000000 )
      
      TRANSLATE_TO( -4.000000  85.000000  0.000000 )
      
      RUN_TIME( 0.100000 )
      
      )
     
     OBJECT_MOTION_FROM_TO(
       NAME(MENURedDown)
      
