     ui::UI_COMMAND(
       NODE(MENULoadingText)
      
      CAPTION(LOADING...)
      
      )
     
     WAIT( 0.200000 )
     
     ui::UI_COMMAND(
       NODE(MENULoadingText)
      
      CAPTION(LOADING....)
      
      )
     
     WAIT( 0.200000 )
     
     ui::UI_COMMAND(
       NODE(MENULoadingText)
      
      CAPTION(LOADING.....)
      
      )
     
     WAIT( 0.200000 )
     
     ui::UI_COMMAND(
       NODE(MENULoadingText)
      
      CAPTION(LOADING.......)
      
      )
     
     WAIT( 0.200000 )
     
     ui::UI_COMMAND(
