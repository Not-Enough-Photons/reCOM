    SEQUENCE_DEFINITION(
      VALVE(AcceptOrDecline= 1  0.000000 )
     
     ui::UI_COMMAND(
       NODE(MENUDescriptionText)
      
      CAPTION("Press the X button to activate the decline invitations list")
      
      )
     
     )
    
    )
   
