        ARGUMENT ( DTV )
       
       )
      
      )
     
      VALVE ( video_mode  =  1 )
     
      ui::UI_COMMAND (
        NODE ( MENU  video_mode )
      
       SELECTION (
         INDEX ( 1 )
       
       )
      
      )
     
      ENDIF ()
     
