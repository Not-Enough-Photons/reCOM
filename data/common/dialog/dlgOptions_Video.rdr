        VALVE ( AreYouSure  ==  0 )
      
      )
     
      END_WHILE ()
     
     )
    
    )
   
    ANIMATION_DEFINITION (
      ANIMATION_NAME ( video_dtv )
    
     ACTIVATION ( ON_CALL )
    
     SEQUENCE_DEFINITION (
       VALVE ( OptionsMenuChanged  =  1 )
     
      IF (
        !  ui::UI_COMMAND (
         TYPE ( CheckVideoMode )
       
        ARGUMENT ( DTV )
       
       )
      
      )
     
      CALL_ANIMATION (
        NAME ( verify_video_dtv )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      IF (
        VALVE ( AreYouSure  ==  1 )
      
      )
     
      VALVE ( video_mode  =  1 )
     
      ui::UI_COMMAND (
        TYPE ( SwitchProgressiveVideo )
      
       ARGUMENT ( DTV )
      
      )
     
      ENDIF ()
     
      ENDIF ()
     
      IF (
        VALVE ( AreYouSure  ==  1 )
      
      )
     
      CALL_SEQUENCE (
        NAME ( monitor_acceptance )
      
      )
     
      CALL_SEQUENCE (
        NAME ( switch_safeguard )
      
      )
     
      ELSE ()
     
      CALL_ANIMATION (
        NAME ( Initialize_Options_Stuff )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      CALL_ANIMATION (
        NAME ( OnVideoModeAct )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      ENDIF ()
     
     )
    
     SEQUENCE_DEFINITION (
       NAME ( monitor_acceptance )
     
      ACTIVATION ( ON_CALL )
     
      VALVE ( AreYouSure  =  0 )
     
      ui::UI_COMMAND (
        TYPE ( POPUPDIALOG )
      
      )
     
