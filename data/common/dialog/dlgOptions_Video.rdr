        NAME ( video_ntsc )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      CALL_ANIMATION (
        NAME ( Initialize_Options_Stuff )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      CALL_ANIMATION (
        NAME ( OnVideoModeAct )
      
       WAIT_FOR_COMPLETION ()
      
      )
     
      VALVE ( AreYouSure  =  0 )
     
