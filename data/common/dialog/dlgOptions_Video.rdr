        TYPE ( DisableButton )
      
       ARGUMENT ( video_mode )
      
      )
     
      IF (
        VALVE ( SUBTITLE_CONTROLLER  ==  1 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  subtitles )
      
       SELECTION (
         INDEX ( 1 )
       
       )
      
      )
     
      ELSE ()
     
      ui::UI_COMMAND (
        NODE ( MENU  subtitles )
      
       SELECTION (
         INDEX ( 0 )
       
       )
      
      )
     
      ENDIF ()
     
      IF (
        VALVE ( BOBBING_1ST_PERSON  ==  1 )
      
      )
     
      ui::UI_COMMAND (
        NODE ( MENU  headbobbing )
      
       SELECTION (
         INDEX ( 1 )
       
       )
      
      )
     
      ELSE ()
     
      ui::UI_COMMAND (
        NODE ( MENU  headbobbing )
      
       SELECTION (
         INDEX ( 0 )
       
       )
      
      )
     
      ENDIF ()
     
      IF (
        ui::UI_COMMAND (
         TYPE ( CheckVideoMode )
       
        ARGUMENT ( NTSC )
       
       )
      
      )
     
      VALVE ( video_mode  =  0 )
     
      ui::UI_COMMAND (
        NODE ( MENU  video_mode )
      
       SELECTION (
         INDEX ( 0 )
       
       )
      
      )
     
      ELSEIF (
