      )
     
      ui::UI_COMMAND (
        NODE ( MENU  ONLINE_LOBBY_STATE )
      
       CAPTION ( "STATE: Login Failure" )
      
      )
     
      VALVE ( AreYouSure  =  0 )
     
      STOP_ANIMATION (
        NAME ( menu_fade_up )
      
      )
     
      OBJECT_OPACITY_FROM_TO (
        NAME ( MENU  MENUFADER )
      
       RUN_TIME ( 0.050000 )
      
       OPACITY_FROM ( 0.000000 )
      
       OPACITY_TO ( 0.700000 )
      
      )
     
      ui::UI_COMMAND (
        TYPE ( POPUPDIALOG )
      
      )
     
