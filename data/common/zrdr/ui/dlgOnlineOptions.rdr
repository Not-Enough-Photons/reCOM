   ANIMATION_DEFINITION(
     ANIMATION_NAME(check_then_save_and_leave)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      IF(
       VALVE(OptionsChanged== 1  0.000000 )
      
      )
     
     CALL_ANIMATION(
       NAME(save_and_leave)
      
      WAIT_FOR_COMPLETION()
      
      )
     
     VALVE(OptionsChanged= 0  0.000000 )
     
     ELSE()
     
     CALL_ANIMATION(
       NAME(menu_fade_down)
      
      )
     
     ui::UI_COMMAND(
       TYPE(SWITCHMENU)
      
      ARGUMENT(dlgWorldOfSOCOM.rdr)
      
      )
     
     ENDIF()
     
     )
    
    )
   
