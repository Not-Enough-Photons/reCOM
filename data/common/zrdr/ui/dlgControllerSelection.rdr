     ui::UI_COMMAND(
       NODE(MENUstick_presets)
      
      SELECTION(
        INDEX( 1  0.000000 )
       
       )
      
      )
     
     ELSEIF(
       VALVE(stick_preset== 1  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUstick_presets)
      
      SELECTION(
        INDEX( 2  0.000000 )
       
       )
      
      )
     
     ELSEIF(
       VALVE(stick_preset== 2  0.000000 )
      
      )
     
     ui::UI_COMMAND(
