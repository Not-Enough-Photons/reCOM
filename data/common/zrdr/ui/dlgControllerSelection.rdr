      SELECTION(
        INDEX( 3  0.000000 )
       
       )
      
      )
     
     ELSE()
     
     VALVE(stick_preset= 3  0.000000 )
     
     ui::UI_COMMAND(
       NODE(MENUstick_presets)
      
      SELECTION(
        INDEX( 0  0.000000 )
       
       )
      
