   ANIMATION_DEFINITION(
     ANIMATION_NAME(Initialize_Options_Stuff)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      ui::UI_COMMAND(
       TYPE(DisableButton)
      
      ARGUMENT(video_mode)
      
      )
     
     IF(
       VALVE(SUBTITLE_CONTROLLER==10.000000)
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUsubtitles)
      
      SELECTION(
        INDEX(10.000000)
       
       )
      
      )
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUsubtitles)
      
      SELECTION(
        INDEX(00.000000)
       
       )
      
      )
     
     ENDIF()
     
     IF(
       VALVE(BOBBING_1ST_PERSON==10.000000)
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUheadbobbing)
      
      SELECTION(
        INDEX(10.000000)
       
       )
      
      )
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUheadbobbing)
      
      SELECTION(
        INDEX(00.000000)
       
       )
      
      )
     
     ENDIF()
     
     IF(
       ui::UI_COMMAND(
        TYPE(CheckVideoMode)
       
       ARGUMENT(NTSC)
       
       )
      
      )
     
     VALVE(video_mode=00.000000)
     
     ui::UI_COMMAND(
       NODE(MENUvideo_mode)
      
      SELECTION(
        INDEX(00.000000)
       
       )
      
      )
     
     ELSEIF(
       ui::UI_COMMAND(
        TYPE(CheckVideoMode)
       
       ARGUMENT(DTV)
       
       )
      
      )
     
     VALVE(video_mode=10.000000)
     
     ui::UI_COMMAND(
       NODE(MENUvideo_mode)
      
      SELECTION(
        INDEX(10.000000)
       
       )
      
      )
     
     ENDIF()
     
     IF(
       VALVE(FirstVideo==00.000000)
      
      )
     
     ui::UI_COMMAND(
       TYPE(ActivateButton)
      
      ARGUMENT(brightnessbar)
      
      )
     
     VALVE(FirstVideo=10.000000)
     
     ENDIF()
     
     )
    
    )
   
