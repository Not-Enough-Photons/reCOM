     IF(
       VALVE(AreYouSure==10.000000)
      
      )
     
     VALVE(video_mode=10.000000)
     
     ui::UI_COMMAND(
       TYPE(SwitchProgressiveVideo)
      
      ARGUMENT(DTV)
      
      )
     
     ENDIF()
     
     ENDIF()
     
