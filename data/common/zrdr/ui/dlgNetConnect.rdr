   ANIMATION_DEFINITION(
     ANIMATION_NAME(init_socom_online)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      ui::UI_COMMAND(
       TYPE(SysPostInit)
      
      )
     
     VALVE(UsagePolicyReturnScreen=10.000000)
     
     IF(
       VALVE(socom_online_initialized==00.000000)
      
      )
     
     VALVE(socom_online_state=00.000000)
     
     IF(
       ui::UI_COMMAND(
        TYPE(INITNETWORK)
       
       )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Waiting to Connect" )
      
      )
     
     WHILE(
       VALVE(socom_online_state==00.000000)
      
      )
     
     END_WHILE()
     
     CALL_SEQUENCE(
       NAME(check_ifconfig)
      
      )
     
     IF(
       VALVE(socom_online_state==10.000000)
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Connected" )
      
      )
     
     VALVE(socom_online_initialized=10.000000)
     
     CALL_SEQUENCE(
       NAME(socom_online_login)
      
      )
     
     ELSEIF(
       VALVE(socom_online_state==20.000000)
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Connect Failure" )
      
      )
     
     CALL_ANIMATION(
       NAME(exit_connect_screen)
      
      )
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Unknown Failure" )
      
      )
     
     CALL_ANIMATION(
       NAME(exit_connect_screen)
      
      )
     
     ENDIF()
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Network Initialization Failure" )
      
      )
     
     WAIT(0.000000)
     
     ui::UI_COMMAND(
       TYPE(SWITCHMENU)
      
      ARGUMENT(PREVIOUS_SCREEN)
      
      )
     
     ENDIF()
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Invalid Initialization State" )
      
      )
     
     WAIT(0.000000)
     
     ui::UI_COMMAND(
       TYPE(SWITCHMENU)
      
      ARGUMENT(PREVIOUS_SCREEN)
      
      )
     
     ENDIF()
     
     )
    
    SEQUENCE_DEFINITION(
      NAME(check_ifconfig)
     
     ACTIVATION(ON_CALL)
     
     WHILE(TRUE)
     
     ui::UI_COMMAND(
       TYPE(SysLoadIRX)
      
      ARGUMENT(run/irx/ifconfig.irx)
      
      )
     
     WAIT(0.000000)
     
     END_WHILE()
     
     )
    
