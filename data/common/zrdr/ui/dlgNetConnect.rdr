   ANIMATION_DEFINITION(
     ANIMATION_NAME(init_socom_online)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      ui::UI_COMMAND(
       TYPE(SysPostInit)
      
      )
     
     VALVE(UsagePolicyReturnScreen= 1  0.000000 )
     
     IF(
       VALVE(socom_online_initialized== 0  0.000000 )
      
      )
     
     VALVE(socom_online_state= 0  0.000000 )
     
     IF(
       ui::UI_COMMAND(
        TYPE(INITNETWORK)
       
       )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Waiting to Connect")
      
      )
     
     WHILE(
       VALVE(socom_online_state== 0  0.000000 )
      
      )
     
     END_WHILE()
     
     CALL_SEQUENCE(
       NAME(check_ifconfig)
      
      )
     
     IF(
       VALVE(socom_online_state== 1  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Connected")
      
      )
     
     VALVE(socom_online_initialized= 1  0.000000 )
     
     CALL_SEQUENCE(
       NAME(socom_online_login)
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 2  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Connect Failure")
      
      )
     
     CALL_ANIMATION(
       NAME(exit_connect_screen)
      
      )
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Unknown Failure")
      
      )
     
     CALL_ANIMATION(
       NAME(exit_connect_screen)
      
      )
     
     ENDIF()
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Network Initialization Failure")
      
      )
     
     WAIT( 1.000000 )
     
     ui::UI_COMMAND(
       TYPE(SWITCHMENU)
      
      ARGUMENT(PREVIOUS_SCREEN)
      
      )
     
     ENDIF()
     
     ELSE()
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Invalid Initialization State")
      
      )
     
     WAIT( 1.000000 )
     
     ui::UI_COMMAND(
       TYPE(SWITCHMENU)
      
      ARGUMENT(PREVIOUS_SCREEN)
      
      )
     
     ENDIF()
     
     )
    
    SEQUENCE_DEFINITION(
      NAME(check_ifconfig)
     
     ACTIVATION(ON_CALL)
     
     WHILE(TRUE)
     
     ui::UI_COMMAND(
       TYPE(SysLoadIRX)
      
      ARGUMENT(run/irx/ifconfig.irx)
      
      )
     
     WAIT( 15.000000 )
     
     END_WHILE()
     
     )
    
    SEQUENCE_DEFINITION(
      NAME(socom_online_login)
     
     ACTIVATION(ON_CALL)
     
     IF(
       ui::UI_COMMAND(
        TYPE(LobbyLogin)
       
       )
      
      )
     
     WHILE(TRUE)
     
     IF(
       VALVE(socom_online_state== 12  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Session Begin")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 7  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Account Login")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 3  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Account Registration")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 8  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: User Login")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 4  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: User Registration")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 9  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Screename Login")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 5  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Screename Registration")
      
      )
     
     ELSEIF(
       VALVE(socom_online_state== 10  0.000000 )
      
      )
     
     ui::UI_COMMAND(
       NODE(MENUONLINE_LOBBY_STATE)
      
      CAPTION("STATE: Login Failure")
      
      )
     
     VALVE(AreYouSure= 0  0.000000 )
     
     STOP_ANIMATION(
       NAME(menu_fade_up)
      
      )
     
     OBJECT_OPACITY_FROM_TO(
       NAME(MENUMENUFADER)
      
      RUN_TIME( 0.050000 )
      
