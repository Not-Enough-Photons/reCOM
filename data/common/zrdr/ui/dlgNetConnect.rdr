       NODE(MENUPOPUP)
      
      CAPTION("Login error. Database does not recognize your entry. Would you like to register this username?")
      
      BACKGROUND(popupOp.tif)
      
      POSITION( 170.000000  240.000000 )
      
      WIDTH( 300  0.000000 )
      
      HEIGHT( 125  0.000000 )
      
      XOFFSET( 20  0.000000 )
      
      YOFFSET( 30  0.000000 )
      
