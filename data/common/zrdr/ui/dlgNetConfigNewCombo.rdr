   ANIMATION_LIST(
    ANIMATION_DEFINITION_FILE(uianim.rdr)
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(on_start)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(DEV_INDEX=00.000000)
     
     VALVE(IFC_INDEX=00.000000)
     
     CALL_ANIMATION(
       NAME(refresh_dev)
      
      )
     
     CALL_ANIMATION(
       NAME(refresh_ifc)
      
      )
     
     )
    
    )
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(incr_dev)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(DEV_INDEX+=10.000000)
     
     IF(
       VALVE(DEV_INDEX>=DEV_COUNT)
      
      )
     
     VALVE(DEV_INDEX=00.000000)
     
     ENDIF()
     
     CALL_ANIMATION(
       NAME(refresh_dev)
      
      )
     
     )
    
    )
   
