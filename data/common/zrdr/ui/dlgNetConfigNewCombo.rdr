 ANIMATION_DEFINITIONS(
   ANIMATION_LIST(
    ANIMATION_DEFINITION_FILE(uianim.rdr)
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(on_start)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(DEV_INDEX= 0  0.000000 )
     
     VALVE(IFC_INDEX= 0  0.000000 )
     
     CALL_ANIMATION(
       NAME(refresh_dev)
      
      )
     
     CALL_ANIMATION(
       NAME(refresh_ifc)
      
      )
     
     )
    
    )
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(incr_dev)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(DEV_INDEX+= 1  0.000000 )
     
     IF(
       VALVE(DEV_INDEX>=DEV_COUNT)
      
      )
     
     VALVE(DEV_INDEX= 0  0.000000 )
     
     ENDIF()
     
     CALL_ANIMATION(
       NAME(refresh_dev)
      
      )
     
     )
    
    )
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(refresh_dev)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      ui::UI_COMMAND(
       TYPE(NetGetDevInfo)
      
      ARGUMENT(DEV_INDEX)
      
      )
     
     )
    
    )
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(incr_ifc)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(IFC_INDEX+= 1  0.000000 )
     
     IF(
       VALVE(IFC_INDEX>=IFC_COUNT)
      
      )
     
     VALVE(IFC_INDEX= 0  0.000000 )
     
     ENDIF()
     
     CALL_ANIMATION(
       NAME(refresh_ifc)
      
      )
     
     )
    
    )
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(refresh_ifc)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      ui::UI_COMMAND(
       TYPE(NetGetIFCInfo)
      
      ARGUMENT(IFC_INDEX)
      
      )
     
     )
    
    )
   
