    SEQUENCE_DEFINITION(
      ui::UI_COMMAND(
       TYPE(NetGetDevInfo)
      
      ARGUMENT(DEV_INDEX)
      
      )
     
     )
    
    )
   
   ANIMATION_DEFINITION(
     ANIMATION_NAME(incr_ifc)
    
    ACTIVATION(ON_CALL)
    
    SEQUENCE_DEFINITION(
      VALVE(IFC_INDEX+=10.000000)
     
     IF(
       VALVE(IFC_INDEX>=IFC_COUNT)
      
